{
  "name": "twitter-dm-bot",
  "version": "1.0.0",
  "description": "A simple bot that sends twitter DMs",
  "author": "Aidan Walz",
  "private": true,
  "dependencies": {
    "express": "^4.17.1",
    "get-port": "^5.1.1",
    "googleapis": "^39.0.0",
    "open": "^8.0.6",
    "puppeteer": "^9.0.0"
  },
  "devDependencies": {
    "pkg": "^5.0.0"
  },
  "engines": {
    "node": ">=6.4.0"
  },
  "pkg": {
    "assets": [
      "assets/*"
    ]
  },
  "bin": "index.js",
  "main": "index.js",
  "scripts": {
    "build": "rm -rf build && pkg . --out-path build",
    "postbuild": "cp -R ./node_modules/puppeteer/.local-chromium build/chromium && cp credentials.json build && cp -R assets build",
    "start": "node ."
  },
  "license": "Apache-2.0"
}
